<div class="contact">
	<% if Phone %><p class="phone"><span>Phone:</span> $Phone</p><% end_if %>
	<% if TollFree %><p class="tollfree"><span>Toll Free:</span> $TollFree</p><% end_if %>
	<% if Mobile %><p class="mobile"><span>Mobile:</span> $Mobile</p><% end_if %>
	<% if Fax %><p class="fax"><span>Fax:</span> $Fax</p><% end_if %>
	<% if Email %><p class="email"><span>Email:</span> <a href="mailto:$Email?subject=Website%20Enquiry">$Email</a></p><% end_if %>
	<% if Postal %><p class="postal"><span>Postal</span> $Postal</p><% end_if %>
	<% if Address %><p class="address"><span>Address</span> $Address</p><% end_if %>
</div>
